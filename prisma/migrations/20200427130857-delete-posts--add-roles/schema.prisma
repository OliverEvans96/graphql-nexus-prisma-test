generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url = "***"
}

model User {
  id         Int      @default(autoincrement()) @id
  createdAt  DateTime @default(now())
  email      String   @unique
  givenName  String
  familyName String
  roles      Role[]
}

enum Role {
  Employee
  Manager
  Admin
}